        function submitOutForm(flagTrans) {
            var f = $("#inputForm").valid();
            if(!f){
                return;
            }
            $("#nextStep").val("end");
            var type=$("#textType").val();
            $("#flagTrans").val(flagTrans);
            if(type == "" || type == '1' || type == '2'){
                //无正文时提示
                var msg = "";
                if(type == ""){
                    msg="未添加正文，无法转出到公文系统，流程将会结束！\r你确定要选择此操作吗？";
                }else if(type == '1' || type == '2'){
                    msg="正文格式不正确(支持：.doc/.docx/.ofd/.pdf/.gw)，无法转出到公文系统，流程将会结束！\r你确定要选择此操作吗？";
                }
                top.$.jBox.confirm(msg, "提示", function(v, h, f){
                    if(v == 'ok'){
                        $("body").mLoading("show");
                        document.getElementById("inputForm").action="${ctx}/${package}/${entityName}/sendToDocSys";
                        $("#inputForm").submit();
                    } else{
                        return;
                    }
                });
            }else{
                $("body").mLoading("show");
                document.getElementById("inputForm").action="${ctx}/${package}/${entityName}/sendToDocSys";
                $("#inputForm").submit();
            }
        }
        // 1.提交表单
        function submitForm(flagTrans) {
            /***start***********************check是否有正在上传的附件**********************start***/
            if(!isUploaded(uploader)){
                showTip("文件正在上传中，请稍后...", "info");
                return;
            }
            var transferOutFlag="${transferOutFlag}";
            if('yes'==transferOutFlag){
                $("#documentType").removeClass("required");
                $("#sealUnitName").removeClass("required");
            }
            /***end*************************check是否有正在上传的附件************************end***/
            if(flagTrans=="yes"){
                cheackForm(flagTrans);
            }else if(flagTrans=="saveData"){
                submitSaveData(flagTrans);
            }
        }
        function changeUtil(){
            var unitType = $("#documentType").val();
            if(unitType == '1'){
                $("#sealUnitName").css("display","block");
                $("#help-inline").css("display","inline-block");
            }else{
                $("#sealUnitName").css("display","none");
                $("#help-inline").css("display","none");
            }
