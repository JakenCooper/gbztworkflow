        function cleanBlankP() { // 清除空P
            $('#mainView p').each(function(index,ele){
                var data = $(ele).html().replace(/&nbsp;/ig, "").replace(/<br>/ig, "").replace(/\s+/g,""); // 先去&nbsp; 再去<br>,最后去空格,若为空则删除
                if(data == null || data == ''){
                    $(ele).remove();
                }
            });
        }

        // 暂存方法
        function temporarySave(yes){
            $('#isTemporarySave').val('true');
            submitForm(yes);
